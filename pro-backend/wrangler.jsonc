{
    "name": "underseerr-notifications",
    "main": "src/index.ts",
    "compatibility_date": "2024-04-03",
    "kv_namespaces": [
        {
            "binding": "TOKENS"
        }
    ],
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "underseerr-billing"
        }
    ],
    "vars": {
        "MODE": "PRO"
    },
    "env": {
        "staging": {
            "name": "underseerr-pro-api-staging",
            "vars": {
                "MODE": "STAGING"
            },
            "kv_namespaces": [
                {
                    "binding": "TOKENS",
                    "id": "PRO_TOKENS_KV_ID_STAGING"
                }
            ],
            "d1_databases": [
                {
                    "binding": "DB",
                    "database_name": "underseerr-pro-api-d1-staging",
                    "database_id": "PRO_BILLING_D1_ID_STAGING"
                }
            ]
        },
        "production": {
            "name": "underseerr-pro-api-prod",
            "vars": {
                "MODE": "PRO"
            },
            "kv_namespaces": [
                {
                    "binding": "TOKENS",
                    "id": "PRO_TOKENS_KV_ID_PROD"
                }
            ],
            "d1_databases": [
                {
                    "binding": "DB",
                    "database_name": "underseerr-pro-api-d1-prod",
                    "database_id": "PRO_BILLING_D1_ID_PROD"
                }
            ]
        }
    }
}