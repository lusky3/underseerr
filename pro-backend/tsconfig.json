{
    "compilerOptions": {
        "target": "es2021",
        "lib": [
            "es2021"
        ],
        "module": "es2022",
        "moduleResolution": "node",
        "types": [
            "@cloudflare/workers-types"
        ],
        "resolveJsonModule": true,
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.js"
    ]
}